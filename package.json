{ "name": "buddycloud-server"
,"version": "0.4.0"
,"bin": { "buddycloud-server": "./bin/buddycloud-server" }
,"description": "buddycloud channels service for XMPP"
,"dependencies": {
        "node-xmpp": ">=0.3.2",
        "node-stringprep": ">=0.1.4",
        "node-expat": ">=1.4.4",
        "ltx": ">=0.1.3",
        "jsconfig": ">=0.2.0",
        "async": ">=0.1.18",
        "node-uuid": ">=1.3.3",
        "pg": ">=0.6.14",
        "underscore.logger": ">=0.3.1",
        "ain2": ">=1.1.0",
        "moment": ">=2.0.0"
        }
,"devDependencies":  {
        "colors": ">=0.6.0-1",
        "tarballify": ">=0.0.5",
        "muffin": ">=0.2.7",
        "coffee-script": ">=1.2.0",
        "mocha": ">=1.4.0",
